# Configuration Supabase
# Copier ce fichier vers .env.local et remplir avec vos vraies valeurs

# URL de votre projet Supabase
VITE_SUPABASE_URL=your_supabase_project_url

# Clé publique anonyme de votre projet Supabase
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optionnel : URL de développement local
# VITE_SUPABASE_URL=http://localhost:54321
# VITE_SUPABASE_ANON_KEY=your_local_anon_key

# Application Configuration
VITE_APP_NAME="Sistema de Control Calidad Microbiológico"
VITE_APP_VERSION="1.0.0"
VITE_APP_COMPANY="MAISON COLLET"

# Development Settings
VITE_DEV_MODE=true
VITE_DEBUG_LOGS=true

# Features Flags
VITE_ENABLE_BACTERIA_SYSTEM=true
VITE_ENABLE_REALTIME_STATS=true
VITE_ENABLE_COLOR_CODING=true

# Demo Accounts (Development Only)
VITE_DEMO_COORDINATOR_EMAIL=demo.coordinator@collet.fr
VITE_DEMO_TECHNICIAN_EMAIL=demo.technician@collet.fr
VITE_DEMO_PASSWORD=demo123 